  "license": "MIT",
  "homepage": "https://github.com/Jeremy-Hibiki/vscode-variable-rw-reference#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jeremy-Hibiki/vscode-variable-rw-reference"
